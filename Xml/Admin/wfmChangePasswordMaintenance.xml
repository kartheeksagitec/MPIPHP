<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmChangePasswordMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="207 " sfwType="Maintenance" sfwTitle="Change Password Maintenance" version="3.5" WebSite="MPIPHPWebClient" sfwEntity="entUser">
