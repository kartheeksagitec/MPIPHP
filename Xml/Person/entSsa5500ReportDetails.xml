    <query ID="rptSSA8955Form" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="DECLARE @DATEOFSSA8955FORM DATETIME&#xD;&#xA;SET @DATEOFSSA8955FORM =  CONVERT(DATETIME,CONVERT(VARCHAR,GETDATE(),101) ,110)&#xD;&#xA;&#xD;&#xA;SELECT  SSA.REPORT_ID,&#xD;&#xA;  SSA.CODE,&#xD;&#xA;  SSA.SSN,&#xD;&#xA;  SSA.NAME,&#xD;&#xA;  SSA.TYPE_OF_ANNUITY,&#xD;&#xA;  SSA.PAYMENT_FREQUENCY,&#xD;&#xA;  ISNULL(SSA.GROSS_PAYMENT,0) AS GROSS_PAYMENT,&#xD;&#xA;  ISNULL(SSA.UNITS_SHARES,0) AS UNITS_SHARES,&#xD;&#xA;  ISNULL(SSA.TOTAL_VALUE_ACCOUNT,0) AS TOTAL_VALUE_ACCOUNT,&#xD;&#xA;  SSA.PLAN_YEAR,&#xD;&#xA;  SSA.VESTED,&#xD;&#xA;  SSA.PLAN_IDENTIFIER,&#xD;&#xA;  SUBSTRING(SP.FIRST_NAME,1,11) AS FIRST_NAME,&#xD;&#xA;  SUBSTRING(SP.MIDDLE_NAME,1,1) AS MIDDLE_INITIAL,&#xD;&#xA;  SUBSTRING(SP.LAST_NAME,1,35) AS LAST_NAME,&#xD;&#xA;  PERADDR.ADDR_LINE_1,&#xD;&#xA;  PERADDR.ADDR_LINE_2,&#xD;&#xA;  PERADDR.ADDR_CITY,&#xD;&#xA;  PERADDR.ADDR_STATE_ID,&#xD;&#xA;  PERADDR.ADDRESS_TYPE,&#xD;&#xA;  PERADDR.ADDR_STATE_VALUE,&#xD;&#xA;  PERADDR.ADDR_COUNTRY_ID,&#xD;&#xA;  PERADDR.ADDR_COUNTRY_VALUE,&#xD;&#xA;  PERADDR.ADDR_ZIP_CODE,&#xD;&#xA;  PERADDR.ADDR_ZIP_4_CODE,&#xD;&#xA;  PERADDR.FOREIGN_PROVINCE,&#xD;&#xA;  PERADDR.FOREIGN_POSTAL_CODE&#xD;&#xA;FROM SGT_SSA_5500_REPORT_DETAILS SSA&#xD;&#xA;INNER JOIN SGT_PERSON SP ON SSA.SSN = SP.SSN&#xD;&#xA;LEFT OUTER JOIN &#xD;&#xA;     (&#xD;&#xA;     SELECT  P.PERSON_ID, &#xD;&#xA;             PA1.ADDR_LINE_1 AS ADDR_LINE_1,&#xD;&#xA;             PA1.ADDR_LINE_2 AS ADDR_LINE_2,&#xD;&#xA;         PA1.ADDR_CITY  AS ADDR_CITY,&#xD;&#xA;         PA1.ADDR_STATE_ID  AS ADDR_STATE_ID,&#xD;&#xA;         CASE WHEN PAC.ADDRESS_TYPE_VALUE='MAIL' OR PAC.ADDRESS_TYPE_VALUE IS NULL  then 1 ELSE 2 END AS ADDRESS_TYPE,&#xD;&#xA;         PA1.ADDR_STATE_VALUE AS ADDR_STATE_VALUE,&#xD;&#xA;            PA1.ADDR_COUNTRY_ID  AS ADDR_COUNTRY_ID,&#xD;&#xA;                                 PA1.ADDR_COUNTRY_VALUE  AS ADDR_COUNTRY_VALUE,&#xD;&#xA;                                 PA1.ADDR_ZIP_CODE AS ADDR_ZIP_CODE,&#xD;&#xA;                                 PA1.ADDR_ZIP_4_CODE  AS ADDR_ZIP_4_CODE,&#xD;&#xA;                                 PA1.FOREIGN_PROVINCE  AS FOREIGN_PROVINCE,&#xD;&#xA;                                 PA1.FOREIGN_POSTAL_CODE  AS FOREIGN_POSTAL_CODE&#xD;&#xA;                           FROM SGT_PERSON P &#xD;&#xA;                           LEFT OUTER JOIN SGT_PERSON_ADDRESS PA1   &#xD;&#xA;                           ON P.PERSON_ID  = PA1.PERSON_ID  &#xD;&#xA;                           --AND DBO.fn_Check_Date_Overlapping(PA1.START_DATE,PA1.END_DATE,'09/01/2012')=1&#xD;&#xA;                           AND PA1.START_DATE &lt;= @DATEOFSSA8955FORM AND (PA1.END_DATE IS NULL OR PA1.END_DATE &gt;= @DATEOFSSA8955FORM)&#xD;&#xA;                           LEFT OUTER JOIN SGT_PERSON_ADDRESS_CHKLIST PAC ON PA1.ADDRESS_ID=PAC.ADDRESS_ID&#xD;&#xA;                           WHERE  PAC.ADDRESS_TYPE_VALUE='MAIL' OR  PAC.ADDRESS_TYPE_VALUE IS NULL&#xD;&#xA;    ) AS PERADDR ON SP.PERSON_ID = PERADDR.PERSON_ID&#xD;&#xA;WHERE SSA.PLAN_YEAR = @PLAN_YEAR AND SSA.PLAN_IDENTIFIER = @PLAN_IDENTIFIER_VALUE    &#xD;&#xA;">
      <parameters>
        <parameter ID="@PLAN_YEAR" />
        <parameter ID="@PLAN_IDENTIFIER_VALUE" />
      </parameters>
    </query>
