<?xml version="1.0" encoding="utf-8"?>
<form ID="rptAgingReport" sfwActive="True" sfwDescription="Aging Report" sfwStatus="Review" sfwResource="200" sfwType="Report" sfwReportName="rptAgingReport.rdlc" sfwReportCategory="Reports" version="4.0">
  <initialload>
    <query sfwQueryRef="entTempdata.rptAgingReport" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwLabel Text="Processed From Date : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="FromDate" sfwDataType="DateTime" />
        <RequiredFieldValidator ControlToValidate="FromDate" ErrorMessage="From Date is Required" ID="rfvFromDate" Text="*" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel Text="Processed To Date : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="ToDate" sfwDataType="DateTime" />
        <CompareValidator ControlToValidate="ToDate" ErrorMessage="To Date must be in mm/dd/yyyy format." ID="valToDate" Operator="DataTypeCheck" Text="*" Type="Date" />
        <CompareValidator ControlToCompare="FromDate" ControlToValidate="ToDate" ErrorMessage="To Date cannot be less than From Date." ID="valDateRange" Operator="GreaterThanEqual" Text="*" Type="Date" />
        <RequiredFieldValidator ControlToValidate="ToDate" ErrorMessage="To Date is Required" ID="rfvToDate" Text="*" />
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwLabel Text="Plan : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn ColumnSpan="3">
        <sfwDropDownList DataTextField="plan_name" DataValueField="plan_id" ID="PLAN_ID" sfwLoadType="Query" sfwLoadSource="entPlan.Lookup" sfwAddEmptyItem="False" />
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <ExtraFields />
</form>