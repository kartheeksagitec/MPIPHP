<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmSSNMergeLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvPerson" sfwResource="301 " sfwType="Lookup" sfwTitle="SSN Merge Lookup" version="3.5" sfwEntity="entPerson" sfwGridCollection="busPersonLookup.iclbPerson">
  <initialload>
    <query ID="cdoPerson" sfwQueryRef="entPerson.Lookup" sfwMethodName="LoadPersons" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtMpiPersonId" ID="capMpiPersonId" Text="MPID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtMpiPersonId" sfwDataField="mpi_person_id" sfwOperator="like" sfwQueryID="cdoPerson" />
                <sfwImageButton ID="btnRetrieve" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtMpiPersonId" sfwSelectedIndex="MpiPersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtSsn" ID="capSsn" Text="SSN : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox CssClass="classFormatSSN" ID="txtSsn" sfwDataField="ssn" sfwQueryID="cdoPerson" SkinID="classFormatSSN" sfwDataFormat="{0:000-##-####}" AllowPartialMask="True" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtLastName" ID="capLastName" Text="Last Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtLastName" sfwDataField="last_name" sfwOperator="like" sfwQueryID="cdoPerson" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtFirstName" ID="capFirstName" Text="First Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtFirstName" sfwDataField="first_name" sfwOperator="like" sfwQueryID="cdoPerson" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtDateOfBirth" ID="capDateOfBirth" Text="Date Of Birth : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtDateOfBirth" sfwDataField="date_of_birth" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="=" sfwQueryID="cdoPerson" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnOpen" sfwActiveForm="wfmSSNMergeMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="astrMPIID=MpiPersonId" sfwRelatedControl="dgrResult" Text="Open" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Person ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PersonId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="MPID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="MpiPersonId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Prefix">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="NamePrefixDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Last Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="LastName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="First Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="FirstName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Suffix">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="NameSuffix" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="SSN">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="IstrSSNNonEncrypted" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Date Of Birth" SortExpressionUpdated="true">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="IdtDateofBirth" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Date Of Death">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="DateOfDeath" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Gender">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="GenderDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Union Code">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="UnionCode" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Employer">
                      <ItemTemplate>
                        <sfwLabel ID="lblLabel" sfwEntityField="IstrEmployerName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Address">
                      <ItemTemplate>
                        <sfwLabel ID="lblLabel1" sfwEntityField="IstrParticipantAddress" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="RelativeVipFlag" sfwShowInExportToExcel="False" Visible="False" ControlStyle.CssClass="hideCintrol">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="IstrRelativeVipFlag" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>