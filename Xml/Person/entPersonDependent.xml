<entity ID="entPersonDependent" sfwActive="True" sfwStatus="Review" sfwType="BusinessRule" sfwMainCDO="icdoRelationship" sfwObjectID="busPersonDependent" sfwParentEntity="entRelationship">
  <rules>
    <rule ID="RULE_CHECK_DEPENDENT_ALREADY_EXISTS" sfwMessageId="1133" sfwExpression="(RFunc.ExecuteQuery&lt;Int32&gt;(&quot;entRelationship.CheckDuplicateDependent&quot;, this.ibusPerson.icdoPerson.person_id, this.icdoRelationship.dependent_person_id) == 1)" sfwObjectBased="True" />
    <rule ID="RULE_CHECK_DEPENDENT_DOB" sfwMessageId="1143" sfwExpression="(this.CheckDependentDOB() == True) &amp;&amp; &#xA;(this.icdoRelationship.relationship_value == &quot;CHLD&quot;)" sfwObjectBased="True" />
    <rule ID="RULE_CHECK_DEPENDENT_ID_IS_BLANK" sfwMessageId="1134" sfwExpression="(string.IsNullOrEmpty(this.istrMpiPersonID))" sfwObjectBased="True" />
    <rule ID="RULE_CHECK_DEPENDENT_IS_UNMARRIED" sfwMessageId="1148" sfwExpression="(this.icdoRelationship.relationship_value == &quot;SPOU&quot;) &amp;&amp; &#xA;(this.CheckDependentMaritalStatus() == True)" sfwObjectBased="True" />
    <rule ID="RULE_CHECK_EFFECTIVE_START_DATE_IS_BLANK" sfwMessageId="1135" sfwExpression="(this.icdoRelationship.effective_start_date == DateTime.MinValue)" sfwObjectBased="True" />
    <rule ID="RULE_CHECK_END_DATE_LESS_THAN_START_DATE" sfwMessageId="1145" sfwExpression="(this.icdoRelationship.effective_end_date != DateTime.MinValue) &amp;&amp; &#xA;(this.icdoRelationship.effective_end_date &lt; this.icdoRelationship.effective_start_date)" sfwObjectBased="True" />
    <rule ID="RULE_CHECK_PERSON_ID_SAME_AS_DEPENDENT_PERSON_ID" sfwMessageId="1132" sfwExpression="(RFunc.GetValue&lt;Int32&gt;(this, &quot;ibusPerson.icdoPerson.person_id&quot;) == this.icdoRelationship.dependent_person_id)" sfwObjectBased="True" />
  </rules>
