<form ID="wfmPersonAccountMaintenance" sfwType="Maintenance" sfwEntity="entPersonAccount" sfwTitle="Person Account Maintenance" sfwActive="True" sfwStatus="Review" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmPersonAccountLookup" sfwRemoteObject="srvPerson">
  <initialload>
    <callmethods sfwMethodName="FindPersonAccountDetail" />
  </initialload>
  <sfwTable>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable ID="toolbar" CellPadding="3">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" Text="Save" sfwNavigationParameter="aintPersonAccountId=PersonAccountId;aintPersonId=objperson.PersonId" sfwCopyActivitySessionVariables="True" sfwExecuteAfterSuccess="btnRefreshData" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
                <sfwButton sfwMethodName="btnValidateExecuteBusinessMethod_Click" Text="Submit" ID="btnSubmit" sfwObjectMethod="BtnSubmitClick" sfwExecuteMethodType="ObjectMethod" sfwVisibleRule="IsSubmitButtonVisible" sfwExecuteAfterSuccess="btnBPMSubmit" />
                <sfwButton sfwMethodName="btnValidateExecuteBusinessMethod_Click" Text="Validate QDRO  Legal Review Required" ID="btnValidateQDROLegalReviewRequired" sfwObjectMethod="ValidateQDROLegalReviewRequired" sfwExecuteMethodType="ObjectMethod" />
                <sfwButton ID="btnOpenLookup" sfwActiveForm="wfmBenefitCalculationLookup" sfwMethodName="btnOpenLookup_Click" sfwNavigationParameter="sp.mpi_person_id=objperson.MpiPersonId;DTL.plan_id=PlanId" Text="Benefit Calculation" sfwCopyActivitySessionVariables="True" OnClientClick="OnclickBenefitCalculation" />
                <sfwButton ID="btnBPMSubmit" sfwRefreshUpdatePanels="uppWorkflowNew;uppCenterMiddle" sfwMethodName="btnBpmSubmit_Click" Text="Submit1" CssClass="hideControl" />
                <sfwButton ID="btnRefreshData" sfwMethodName="btnCancel_Click" Text="Refresh Data" sfwMessageId="8" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Person Account Details">
          <sfwTable ID="tblMain" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capMpiPersonId" Text="MPID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLinkButton ID="btnMpiPersonId" sfwActiveForm="wfmPersonMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintpersonid=PersonId" sfwEntityField="objperson.MpiPersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPersonAccountId" sfwEntityField="PersonAccountId" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capPlan" sfwIsCaption="True" Text="Plan : " />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPlanDescription" sfwEntityField="IstrPlan" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPersonId" sfwEntityField="objperson.PersonId" CssClass="hideControl" />
                <sfwLabel ID="lblMPIPersonId" sfwEntityField="objperson.MpiPersonId" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPlanId" sfwEntityField="PlanId" CssClass="hideControl" />
                <sfwLabel ID="lblActivityInstanceId" sfwEntityField="IintActivityInstanceId" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlServiceRetirement" sfwCaption="Service Retirement Process Details">
          <sfwTable ID="tblServiceRetirement" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capApplicationFormSent" Text="Application Form Sent :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkApplicationFormSent" sfwEntityField="ApplicationFormSent" sfwDefaultValue="N" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capSignedApplicationReceivedDate" Text="Signed Application Form Received Date:" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtSignedApplicationReceivedDate" sfwEntityField="SignedApplicationFornReceivedDate" sfwTwoWayBinding="False" sfwDataFormat="{0:d}" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capQDROLegalReview" Text="QDRO Legal Review Required :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkQDROLegalReview" sfwEntityField="QdroLegalReviewRequired" sfwDefaultValue="N" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capQDROReviewDate" Text="QDRO Review Completed Date :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtQDROReviewDate" sfwEntityField="QdroReviewCompletedDate" sfwTwoWayBinding="False" sfwDataFormat="{0:d}" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capElectionPacketSent" Text="Election Packet Sent :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkElectionPacketSent" sfwEntityField="ElectionPacketSent" sfwDefaultValue="N" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capQDROAuditorName" Text="QDRO Auditor Name :" sfwIsCaption="True" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblQDROAuditorName" sfwEntityField="QdroAuditorName" sfwDefaultValue="N" sfwTwoWayBinding="True" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capElectionPacketReceived" Text="Election packet Received:" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkElectionPacketReceived" sfwEntityField="ElectionPacketReceived" sfwDefaultValue="N" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable ID="tblAuditInfo" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Created : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwEntityField="UpdateSeq" sfwTwoWayBinding="True" Visible="False" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modified : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <ExtraFields />
  <loaddetails>
    <entityfield ID="objperson" sfwNewAutoLoad="True" sfwUpdateAutoLoad="True" />
  </loaddetails>
</form>