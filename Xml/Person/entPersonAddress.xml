    <query ID="GetPayeeAccountsForAddingNewAddress" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT &#xD;&#xA; SPA.PAYEE_ACCOUNT_ID, SPAS.STATUS_VALUE &#xD;&#xA;FROM &#xD;&#xA; SGT_PAYEE_ACCOUNT SPA WITH(NOLOCK)&#xD;&#xA; INNER JOIN SGT_PAYEE_ACCOUNT_STATUS SPAS WITH(NOLOCK) &#xD;&#xA; ON SPA.PAYEE_ACCOUNT_ID = SPAS.PAYEE_ACCOUNT_ID AND SPAS.PAYEE_ACCOUNT_STATUS_ID IN&#xD;&#xA; (SELECT TOP 1 Q.PAYEE_ACCOUNT_STATUS_ID FROM SGT_PAYEE_ACCOUNT_STATUS Q WITH(NOLOCK) &#xD;&#xA; WHERE Q.PAYEE_ACCOUNT_ID = SPA.PAYEE_ACCOUNT_ID &#xD;&#xA; ORDER BY Q.PAYEE_ACCOUNT_STATUS_ID DESC)&#xD;&#xA; AND SPAS.STATUS_VALUE NOT IN ('CMPL', 'CNCL') &#xD;&#xA;WHERE &#xD;&#xA; SPA.PERSON_ID = @PERSON_ID ">
