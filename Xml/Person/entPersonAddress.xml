    <query ID="GetParticipantAddress" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT top(1) PA.*,&#xD;&#xA;'C/O ' + UPPER(ISNULL(RTRIM(P.FIRST_NAME) +' ','') + ISNULL(RTRIM(LTRIM(P.MIDDLE_NAME)) +' ','') + ISNULL(RTRIM(LTRIM(P.LAST_NAME)),'')) AS CO&#xD;&#xA;FROM SGT_PERSON P WITH(NOLOCK)&#xD;&#xA;LEFT OUTER JOIN SGT_PERSON_ADDRESS PA WITH(NOLOCK) ON P.PERSON_ID = PA.PERSON_ID&#xD;&#xA;LEFT OUTER JOIN SGT_PERSON_ADDRESS_CHKLIST PAC ON PA.ADDRESS_ID=PAC.ADDRESS_ID AND PAC.ADDRESS_TYPE_VALUE='MAIL' &#xD;&#xA;WHERE P.PERSON_ID = @PERSON_ID&#xD;&#xA;and pa.END_DATE is null&#xD;&#xA;order by pa.ADDRESS_ID desc">
      <parameters>
