    <query ID="GetPersonAddressForBatch" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="Select * from SGT_PERSON PER&#xD;&#xA;INNER JOIN SGT_PERSON_ADDRESS PAD ON PER.PERSON_ID=PAD.PERSON_ID&#xD;&#xA;INNER JOIN SGT_PERSON_ADDRESS_CHKLIST PADC ON PADC.ADDRESS_ID=PAD.ADDRESS_ID AND PADC.ADDRESS_TYPE_VALUE='MAIL'&#xD;&#xA;WHERE PAD.START_DATE=@START_DATE">
      <parameters>
