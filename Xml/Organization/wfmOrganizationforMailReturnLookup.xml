<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmOrganizationforMailReturnLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvOrganization" sfwResource="300 " sfwType="Lookup" sfwTitle="Organization Lookup" version="3.5" sfwEntity="entOrganization" sfwGridCollection="busOrganizationLookup.iclbOrganization">
  <initialload>
    <query ID="cdoOrganization" sfwQueryRef="entOrganization.LooupForReturnMail" sfwMethodName="LoadOrganizationsReturnedMail" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtMpiOrgId" ID="capMpiOrgId" Text="MPI Organization ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtMpiOrgId" sfwDataField="mpi_org_id" sfwOperator="like" sfwQueryID="cdoOrganization" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtOrgName" ID="capOrgName" Text="Organization Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgName" sfwDataField="org_name" sfwOperator="like" sfwQueryID="cdoOrganization" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblLabel" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel AssociatedControlID="ddlOrgTypeValue" ID="capOrgTypeValue" Text="Organization Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlOrgTypeValue" sfwDataField="org_type_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwLoadType="CodeGroup" sfwLoadSource="201" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtPhoneNo" ID="capPhoneNo" Text="Phone Number : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPhoneNo" sfwDataField="phone_no" sfwDataFormat="{0:(###)###-####}" sfwOperator="like" sfwQueryID="cdoOrganization" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="ddlStatusValue" ID="capStatusValue" Text="Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlStatusValue" sfwDataField="status_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwLoadType="CodeGroup" sfwLoadSource="10" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtRoutingNumber" ID="capRoutingNumber" Text="Routing Number : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtRoutingNumber" sfwDataField="routing_number" sfwOperator="like" sfwQueryID="cdoOrganization" sfwTwoWayBinding="False" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblAdd1" Text="Address Line 1" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTextBox" sfwDataField="addr_line_1" sfwQueryID="cdoOrganization" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lbladd2" Text="Address Line 2" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTextBox1" sfwDataField="addr_line_2" sfwQueryID="cdoOrganization" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblcity1" Text="City" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTextBox2" sfwDataField="city" sfwQueryID="cdoOrganization" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblLabel2" Text="State : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlDropDownList" sfwDataField="state_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwLoadType="CodeGroup" sfwLoadSource="150" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblLabel3" Text="Country: " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlDropDownList1" sfwDataField="country_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwLoadType="CodeGroup" sfwLoadSource="151" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblforie" Text="Foreign Province" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTextBox3" sfwDataField="foreign_province" sfwQueryID="cdoOrganization" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblforiec" Text="Foreign Postal Code" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTextBox4" sfwDataField="foreign_postal_code" sfwQueryID="cdoOrganization" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgId" sfwDataField="org_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoOrganization" Visible="False" />
              </sfwColumn>
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmOrganizationMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="astrOrgType=ddlOrgTypeValue" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmOrganizationMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintorgid=OrgId" sfwRelatedControl="dgrResult" Text="Open" Visible="False" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="OrgId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="MPI Organization ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="MpiOrgId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Organization Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OrgName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Address">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrgAddress.IstrFullAddress" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Phone No">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:(###)###-####}" sfwEntityField="PhoneNo" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="City">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrgAddress.City" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="State">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrgAddress.StateDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Foreign Postal Code">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrgAddress.ForeignPostalCode" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Country">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrgAddress.CountryDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Start Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objOrgAddress.StartDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="End Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objOrgAddress.EndDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Foreign Province">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrgAddress.ForeignProvince" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>