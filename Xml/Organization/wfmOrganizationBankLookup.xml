            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtBankOrgId" ID="capBankOrgId" Text="Bank Org ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtBankOrgId" sfwDataField="bank_org_id" sfwQueryID="cdoOrgBank" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtOrgBankRoutingNo" ID="capRoutingNumber" Text="Routing Number" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgBankRoutingNo" sfwDataField="org_bank_routing_no" sfwOperator="like" sfwQueryID="cdoOrgBank" />
