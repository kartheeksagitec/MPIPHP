              <sfwColumn>
                <sfwLabel AssociatedControlID="txtOrgBankId" ID="capOrgBankId" Text="Org Bank ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgBankId" sfwDataField="org_bank_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoOrgBank" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtAccountNo" ID="capAccountNo" Text="Account No : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtAccountNo" sfwDataField="account_no" sfwOperator="like" sfwQueryID="cdoOrgBank" />
