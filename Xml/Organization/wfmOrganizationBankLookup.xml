<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmOrganizationBankLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvOrganization" sfwResource="300 " sfwType="Lookup" sfwTitle="Organization Bank Lookup" version="3.5" WebSite="MPIPHPWebClient" sfwEntity="entOrgBank" sfwGridCollection="busOrganizationBankLookup.iclbOrgBank">
