<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmBenefitInterestRateLookup" WebSite="MPIPHPWebClient" sfwResource="210 " sfwRemoteObject="srvCalculation" sfwType="Lookup" sfwTitle="Benefit Interest Rate Lookup" sfwActive="True" sfwStatus="Review" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmBenefitInterestRateLookup" sfwEntity="entBenefitInterestRate" sfwGridCollection="busBenefitInterestRateLookup.iclbBenefitInterestRate">
  <initialload>
    <query ID="cdoBenefitInterestRate" sfwQueryRef="entBenefitInterestRate.Lookup" sfwMethodName="LoadBenefitInterestRates" />
  </initialload>
  <sfwTable>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capYear" Text="Year : " AssociatedControlID="txtYear" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtYear" sfwQueryID="cdoBenefitInterestRate" sfwDataField="year" sfwOperator="=" sfwDataType="Numeric" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" sfwOperateOn="tblCriteria" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" sfwOperateOn="tblCriteria" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" sfwOperateOn="tblCriteria" Text="Store Search" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable ID="tblResult" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnExportExcel" ImageUrl="~/Image/gExport.gif" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn CssClass="BenIntRateLookup">
                <sfwGridView ID="dgrResult" AllowPaging="True" AllowSorting="True" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Year" ItemStyle.HorizontalAlign="Left">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="Year" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Rate Of Interest" ItemStyle.HorizontalAlign="Left">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="RateOfInterest" sfwDataFormat="{0:N6}" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>