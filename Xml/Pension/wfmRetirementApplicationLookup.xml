              <sfwColumn>
                <sfwTextBox CssClass="textBoxDisplayStyleNone" ID="txtDroApplicationId" sfwDataField="dro_application_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoBenefitApplication" sfwTwoWayBinding="False" SkinID="textBoxDisplayStyleNone" />
