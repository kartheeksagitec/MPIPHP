                <CompareValidator ControlToCompare="txtApplicationReceivedDate" ControlToValidate="txtApplicationReceivedDate1" ErrorMessage="Application Recieved Date To cannot be less than Application Recieved Date From." ID="valtApplRecvDate1Range" Operator="GreaterThanEqual" Text="*" Type="Date" />
