<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmRetirementApplicationLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvPerson" sfwResource="200" sfwType="Lookup" sfwTitle="Retirement Application Lookup" version="3.5" sfwEntity="entBenefitApplication" sfwGridCollection="busBenefitApplicationLookup.iclbBenefitApplication">
