              <sfwColumn>
                <sfwLabel AssociatedControlID="txtApplicationReceivedDate1" ID="capApplicationReceivedDate1" Text="Application Received Date To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtApplicationReceivedDate1" sfwDataField="application_received_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoBenefitApplication" sfwRelatedControl="txtApplicationReceivedDate" sfwTwoWayBinding="False" />
