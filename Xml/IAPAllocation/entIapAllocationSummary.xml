    <query ID="GetMDDateForIAPAlloactionYerlyDetail" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT * FROM SGT_BENEFIT_APPLICATION SBA WITH(NOLOCK)&#xD;&#xA;INNER JOIN SGT_BENEFIT_APPLICATION_DETAIL SBAD WITH(NOLOCK) ON SBAD.BENEFIT_APPLICATION_ID = SBA.BENEFIT_APPLICATION_ID&#xD;&#xA;INNER JOIN SGT_PERSON SP WITH(NOLOCK) ON SP.PERSON_ID = SBA.PERSON_ID&#xD;&#xA;INNER JOIN SGT_PLAN_BENEFIT_XR PBXR WITH(NOLOCK) ON PBXR.PLAN_BENEFIT_ID = SBAD.PLAN_BENEFIT_ID&#xD;&#xA;WHERE &#xD;&#xA;ISNULL(SBA.MIN_DISTRIBUTION_FLAG,'N') = 'Y'&#xD;&#xA;AND PBXR.PLAN_ID = 2&#xD;&#xA;AND SP.PERSON_ID NOT IN (SELECT SB.PERSON_ID FROM SGT_BENEFIT_APPLICATION SB WHERE SB.BENEFIT_TYPE_VALUE IN ('RTMT', 'DSBL') AND SB.APPLICATION_STATUS_VALUE = 'APPR' &#xD;&#xA;      AND ISNULL(SB.MIN_DISTRIBUTION_FLAG,'N') = 'N' AND SB.PERSON_ID = @PERSON_ID)&#xD;&#xA;AND SP.PERSON_ID = @PERSON_ID">
      <parameters>
        <parameter ID="@PERSON_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="GetIAPSummaryWorkHistory" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT &#x9;LATE_FLAG,&#xD;&#xA;&#x9;&#x9;SUM(IAPHOURS) AS TOTAL_IAP_HOURS,&#xD;&#xA;&#x9;&#x9;SUM(iaphoursa2) AS TOTAL_THIRTY_AND_HALF_HOURS,&#xD;&#xA;&#x9;&#x9;SUM(iaphoursa2 * .305) AS TOTAL_HOURLY_CONTRIBUTION, &#xD;&#xA;&#x9;&#x9;SUM(iappercent) AS TOTAL_PERCENT_OF_COMPENSATION&#xD;&#xA;FROM SGT_ALL_IAP_WORKHISTORY_4_SNAPSHOT&#xD;&#xA;GROUP BY LATE_FLAG">
      <parameters />
    </query>
  </queries>
