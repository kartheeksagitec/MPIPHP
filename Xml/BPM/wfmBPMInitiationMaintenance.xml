<form ID="wfmBPMInitiationMaintenance" sfwActive="True" sfwStatus="Valid" sfwType="Maintenance" sfwTitle="BPM Online Case Intiation" version="3.5" sfwEntity="entSolBpmRequest" sfwResource="20007004">
  <initialload>
    <callmethods sfwMethodName="NewBpmRequest" sfwMode="New" />
    <query />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMessageId="1556" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwObjectMethod="InitializeProcess" Text="Initiate Process" sfwSecurityLevel="2" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Request Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblLabel" CssClass="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel CssClass="LeftAlignedLabel" ID="capProcessId" SkinID="LeftAlignedLabel" Text="Process Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="name" DataValueField="process_id" ID="ddlProcessId" AutoPostBack="True" sfwLoadType="Query" sfwLoadSource="entBpmProcess.GetProcessesForOnlineCaseInitiation" sfwEntityField="ProcessId" />
                <sfwButton ID="btnRefreshParameters" sfwMethodName="btnValidateExecuteBusinessMethod_Click" Text="Refresh Parameters" sfwObjectMethod="LoadRequestParametersForOnlineInitiation" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capPersonId" SkinID="LeftAlignedLabel" Text="MPID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonId" sfwDataField="sp.person_id" sfwOperator="like" sfwQueryID="cdoPayeeAccount" sfwEntityField="objPerson.MpiPersonId" sfwRetrievalQuery="entPerson.GetPersonDetails" sfwParameters="MPI_PERSON_ID=txtPersonId" sfwRetrievalControls="txtPersonID=person_id;lblPersonName=fullname" />
                <sfwTextBox ID="txtPersonID" sfwEntityField="PersonId" sfwTwoWayBinding="False" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capPersonName" SkinID="LeftAlignedLabel" Text="Person Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPersonName" sfwEntityField="objPerson.IstrPersonName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capOrgId" SkinID="LeftAlignedLabel" Text="Organization ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgId" sfwParameters="ORG_ID=txtOrgId" sfwRetrievalControls="lblOrgNameRet=org_name" sfwRetrievalQuery="entOrganization.GetOrgFromOrgID" sfwDataType="Numeric" sfwEntityField="OrgId" />
                <sfwImageButton ID="btnRetrieveOrganization" ImageAlign="AbsMiddle" ImageUrl="~/Images/Lookup.jpg" sfwActiveForm="wfmOrganizationLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtOrgId" sfwSelectedIndex="OrgId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capOrgName" SkinID="LeftAlignedLabel" Text="Organization Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblOrgNameRet" sfwEntityField="objOrganization.OrgName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capReferenceId" SkinID="LeftAlignedLabel" Text="Reference ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtReferenceId" sfwEntityField="ReferenceId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblLabel1" CssClass="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel AssociatedControlID="txtIstrNotes" CssClass="LeftAlignedLabel" ID="capIstrNotes" SkinID="LeftAlignedLabel" Text="Notes :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtIstrNotes" sfwEntityField="IstrNotes" TextMode="MultiLine" Rows="4" Height="400" Width="400px" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capIstrOverrideCheckInstance" Text="Allow Multiple Instances" AssociatedControlID="chkIstrOverrideCheckInstance" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkIstrOverrideCheckInstance" sfwEntityField="CheckForExistingInst" sfwValueChecked="N" sfwValueUnChecked="Y" sfwDefaultValue="Y" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlNewPanel" sfwCaption="Request Parameters">
          <sfwTable CssClass="Table" ID="tblNewPage">
            <sfwRow>
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvbusSolBpmRequestParameter" sfwTwoWayBinding="True" AllowEditing="True" sfwEntityField="lstBpmRequestParameter" sfwGridEntity="entBpmRequestParameter">
                  <Columns>
                    <TemplateField HeaderText="Parameter Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ParameterName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Parameter">
                      <ItemTemplate>
                        <sfwTextBox sfwEntityField="ParameterValue" sfwVisibleRule="Visible Rule - ShowTextBox" />
                        <sfwDropDownList sfwEntityField="ParameterValue" sfwLoadType="Query" sfwVisibleRule="Visible Rule - ShowDropDown" sfwLoadSource="entFramework.GetCodeValuesForMapParameters" sfwNavigationParameter="code_id=CodeGroupId" DataTextField="description" DataValueField="code_value" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Request Parameter ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="RequestParameterId" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="objPerson" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objOrganization" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstBpmRequestParameter" sfwNewAutoLoad="True" sfwUpdateAutoLoad="True" />
  </loaddetails>
</form>