<form ID="wfmAuditLogDetailMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAudit" sfwResource="200" sfwType="Maintenance" sfwTitle="Audit History" version="3.5" WebSite="MPIPHPWebClient" sfwEntity="entMPIFullAudit">
  <initialload>
    <callmethods sfwMethodName="GetFullAuditLog" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar" Visible="False">
          <sfwTable ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" Text="Save" Visible="False" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
                <sfwButton ID="btnCorrespondence" sfwMethodName="btnCorrespondence_Click" sfwResource="335" Text="Correspondence" Visible="False" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlNewPage" sfwCaption="Pesron Detail">
          <sfwTable CssClass="Table" ID="tblNewPage">
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="lblMpiPersonId" ID="capMpiPersonId" Text="MPID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblMpiPersonId" sfwEntityField="objPerson.MpiPersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="lblIstrFullName" ID="capIstrFullName" Text="Participant Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblIstrFullName" sfwEntityField="objPerson.IstrFullName" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Audit History">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrHdr1" PageSize="5" sfwEntityField="lstFullAuditLog" AllowEditing="True" sfwTwoWayBinding="True">
                  <Columns>
                    <TemplateField HeaderText="Audit Details">
                      <ItemTemplate>
                        <sfwJSONData ID="jdataJSONData" sfwEntityField="AuditDetails" Font.Bold="True" Font.Underline="True" ForeColor="blue" Font.Size="Medium" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified By">
                      <ItemTemplate>
                        <sfwLabel ID="lblLabel" sfwEntityField="IstrMODIFIEDBY" sfwTwoWayBinding="True" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified Date">
                      <ItemTemplate>
                        <sfwLabel ID="lblLabel1" sfwEntityField="IdtMODIFIEDDATE" sfwTwoWayBinding="True" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information" Visible="False">
          <sfwTable CssClass="Table" ID="tblAuditInfo">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Create : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modify : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="objPerson" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstFullAuditLog" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
</form>