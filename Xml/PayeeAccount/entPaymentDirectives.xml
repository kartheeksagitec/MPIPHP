    <attribute sfwValue="retirement_type_value" sfwType="Column" sfwKeyNo="0" sfwIsIdentity="False" sfwDataType="string" sfwDBDataType="varchar" sfwLength="4" sfwIsNull="True" ID="RetirementTypeValue" sfwCaption="Retirement Type" />
