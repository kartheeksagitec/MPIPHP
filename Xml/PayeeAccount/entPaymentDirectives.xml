    <attribute sfwValue="payment_end_date" sfwType="Column" sfwKeyNo="0" sfwIsIdentity="False" sfwDataType="datetime" sfwDBDataType="datetime" sfwIsNull="True" ID="PaymentEndDate" sfwCaption="Payment End Date" />
