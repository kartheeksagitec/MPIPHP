<entity ID="entPaymentDirectives" sfwMainCDO="icdoPaymentDirectives" sfwType="BusinessRule" sfwActive="True" sfwStatus="Review" sfwObjectID="busPaymentDirectives" sfwTableName="sgt_payment_directives" sfwView="False" sfwDataObjectID="doPaymentDirectives">
  <queries>
    <query ID="LookUp" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="select * from sgt_payment_directives ">
      <parameters />
    </query>
    <query ID="GetDistributionCodeForPaymentDirectives" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql=" SELECT DISTINCT &#xD;&#xA; CASE WHEN (PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'DSBL' OR (BADSBL.BENEFIT_APPLICATION_ID IS NOT NULL AND BADSBL.DISABILITY_CONVERSION_DATE IS NOT NULL AND BADSBL.RETIREMENT_DATE&lt;=@PAYMENT_DATE)&#xD;&#xA;         OR ((PA.BENEFIT_ACCOUNT_TYPE_VALUE='QDRO' AND DAPP.IS_PARTICIPANT_DISABLED_FLAG='Y') OR (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND BA.DRO_APPLICATION_ID IS NOT NULL AND DRO.IS_PARTICIPANT_DISABLED_FLAG='Y')))&#xD;&#xA;          THEN '3' &#xD;&#xA;          &#xD;&#xA;          &#xD;&#xA;    WHEN (PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'DDPR' OR PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'DDPT') &#xD;&#xA;          THEN '4' + CASE WHEN (PBXR.BENEFIT_OPTION_VALUE = 'LUMP' AND dbo.fn_GET_DECRYPTED_VALUE(PARTI_PER.DATE_OF_BIRTH) &lt;= '01/01/1936')   &#xD;&#xA;                          THEN 'A'&#xD;&#xA;                          WHEN ROLL.ROLLOVER_FLAG ='Y'&#xD;&#xA;                          THEN 'G'&#xD;&#xA;                          ELSE ''&#xD;&#xA;                          END&#xD;&#xA;     &#xD;&#xA;       WHEN (((((PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'WDRL' AND BA.DRO_APPLICATION_ID IS NULL ) OR PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'RTMT') AND (DATEDIFF(D,dbo.fn_GET_DECRYPTED_VALUE(PARTI_PER.DATE_OF_BIRTH),@PAYMENT_DATE))/365.25 &gt;= 59.5)&#xD;&#xA;                    OR ((PA.BENEFIT_ACCOUNT_TYPE_VALUE='QDRO' OR (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND BA.DRO_APPLICATION_ID IS NOT NULL)) AND DATEDIFF(D,dbo.fn_GET_DECRYPTED_VALUE(PER.DATE_OF_BIRTH),@PAYMENT_DATE)/365.25 &gt;= 59.5))&#xD;&#xA;              AND ROLL.ROLLOVER_FLAG ='N') &#xD;&#xA;        THEN '7' +  CASE WHEN (PBXR.BENEFIT_OPTION_VALUE = 'LUMP' AND  ((((PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'WDRL' AND BA.DRO_APPLICATION_ID IS NULL) OR PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'RTMT') AND dbo.fn_GET_DECRYPTED_VALUE(PARTI_PER.DATE_OF_BIRTH) &lt;= '01/01/1936') &#xD;&#xA;                              OR ((PA.BENEFIT_ACCOUNT_TYPE_VALUE='QDRO' OR (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND BA.DRO_APPLICATION_ID IS NOT NULL)) AND dbo.fn_GET_DECRYPTED_VALUE(PER.DATE_OF_BIRTH) &lt;= '01/01/1936')))&#xD;&#xA;                   THEN 'A'&#xD;&#xA;                   ELSE ''&#xD;&#xA;                   END                    &#xD;&#xA;          &#xD;&#xA;    WHEN (((PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'RTMT' OR (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND BA.DRO_APPLICATION_ID IS NULL AND PBXR.BENEFIT_OPTION_VALUE!='LUMP') AND (DATEDIFF(D,dbo.fn_GET_DECRYPTED_VALUE(PARTI_PER.DATE_OF_BIRTH),@PAYMENT_DATE))/365.25 &lt; 59.5)  &#xD;&#xA;     OR ((PA.BENEFIT_ACCOUNT_TYPE_VALUE = 'QDRO' OR (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND BA.DRO_APPLICATION_ID IS NOT NULL AND PBXR.BENEFIT_OPTION_VALUE!='LUMP')) AND (DATEDIFF(D,dbo.fn_GET_DECRYPTED_VALUE(PER.DATE_OF_BIRTH),@PAYMENT_DATE))/365.25 &lt; 59.5)&#xD;&#xA;           OR (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND (SELECT COUNT(*)  FROM SGT_PAYEE_ACCOUNT B INNER JOIN SGT_PLAN_BENEFIT_XR A ON A.PLAN_BENEFIT_ID=B.PLAN_BENEFIT_ID&#xD;&#xA;                                                               WHERE B.PERSON_ID=PA.PERSON_ID AND B.PAYEE_ACCOUNT_ID!=PA.PAYEE_ACCOUNT_ID AND A.PLAN_ID = PBXR.PLAN_ID AND B.BENEFIT_ACCOUNT_TYPE_VALUE!=PA.BENEFIT_ACCOUNT_TYPE_VALUE) &gt; 0)) &#xD;&#xA;           AND ROLL.ROLLOVER_FLAG='N')&#xD;&#xA;          THEN '2'&#xD;&#xA;         &#xD;&#xA;    WHEN ROLL.ROLLOVER_FLAG='Y'      &#xD;&#xA;         THEN 'G'&#xD;&#xA;     &#xD;&#xA;    WHEN (PA.BENEFIT_ACCOUNT_TYPE_VALUE='WDRL' AND PBXR.BENEFIT_OPTION_VALUE='LUMP' AND ROLL.ROLLOVER_FLAG='N' AND (DATEDIFF(D,dbo.fn_GET_DECRYPTED_VALUE(PARTI_PER.DATE_OF_BIRTH),@PAYMENT_DATE))/365.25 &lt; 59.5)     &#xD;&#xA;          THEN '1'      &#xD;&#xA;                   &#xD;&#xA;    ELSE 'ZZ'                                                     &#xD;&#xA;    END &#xD;&#xA; AS DISTRIBUTION_CODE&#xD;&#xA; FROM SGT_PAYEE_ACCOUNT PA WITH(NOLOCK) &#xD;&#xA; INNER JOIN SGT_PLAN_BENEFIT_XR PBXR WITH(NOLOCK) ON PBXR.PLAN_BENEFIT_ID=PA.PLAN_BENEFIT_ID&#xD;&#xA; INNER JOIN SGT_PAYEE_BENEFIT_ACCOUNT PBACC WITH(NOLOCK) ON PBACC.PAYEE_BENEFIT_ACCOUNT_ID=PA.PAYEE_BENEFIT_ACCOUNT_ID&#xD;&#xA; INNER JOIN SGT_PERSON PARTI_PER WITH(NOLOCK) ON PARTI_PER.PERSON_ID = PBACC.PERSON_ID&#xD;&#xA; INNER JOIN&#xD;&#xA; (&#xD;&#xA; SELECT DISTINCT SPA.PAYEE_ACCOUNT_ID,CASE WHEN RD.PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID &gt; 0 THEN 'Y' ELSE 'N' END AS ROLLOVER_FLAG FROM SGT_PAYEE_ACCOUNT SPA&#xD;&#xA; LEFT OUTER JOIN SGT_PAYEE_ACCOUNT_ROLLOVER_DETAIL RD ON SPA.PAYEE_ACCOUNT_ID = RD.PAYEE_ACCOUNT_ID AND STATUS_VALUE = 'ACTV' &#xD;&#xA; )ROLL ON PA.PAYEE_ACCOUNT_ID = ROLL.PAYEE_ACCOUNT_ID&#xD;&#xA; LEFT OUTER JOIN SGT_BENEFIT_APPLICATION BADSBL WITH(NOLOCK) ON BADSBL.PERSON_ID = PA.PERSON_ID AND BADSBL.BENEFIT_TYPE_VALUE='DSBL' AND BADSBL.APPLICATION_STATUS_VALUE='APRD'&#xD;&#xA; LEFT OUTER JOIN SGT_BENEFIT_APPLICATION_DETAIL BAD WITH(NOLOCK) ON BAD.BENEFIT_APPLICATION_DETAIL_ID = PA.BENEFIT_APPLICATION_DETAIL_ID&#xD;&#xA; LEFT OUTER JOIN SGT_BENEFIT_APPLICATION BA WITH(NOLOCK) ON BAD.BENEFIT_APPLICATION_ID = BA.BENEFIT_APPLICATION_ID AND BA.APPLICATION_STATUS_VALUE='APRD'&#xD;&#xA; LEFT OUTER JOIN SGT_DRO_BENEFIT_DETAILS DBD WITH(NOLOCK) ON DBD.DRO_BENEFIT_ID = PA.DRO_APPLICATION_DETAIL_ID&#xD;&#xA; LEFT OUTER JOIN SGT_DRO_APPLICATION DAPP WITH(NOLOCK) ON DAPP.DRO_APPLICATION_ID = DBD.DRO_APPLICATION_ID AND DAPP.DRO_STATUS_VALUE='QLFD'&#xD;&#xA; LEFT OUTER JOIN SGT_DRO_APPLICATION DRO WITH(NOLOCK) ON DRO.DRO_APPLICATION_ID=BA.DRO_APPLICATION_ID AND DRO.DRO_STATUS_VALUE='QLFD'&#xD;&#xA; LEFT OUTER JOIN SGT_PERSON PER WITH(NOLOCK) ON PA.PERSON_ID = PER.PERSON_ID&#xD;&#xA; WHERE PA.PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID">
      <parameters>
        <parameter ID="@PAYMENT_DATE" sfwDataType="DateTime" />
        <parameter ID="@PAYEE_ACCOUNT_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="GetEEUVHPAmountForPaymentDirectives" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT SPA.PAYEE_ACCOUNT_ID,ISNULL(NON_VESTED_EE_AMOUNT,0) AS NON_VESTED_EE_AMOUNT,ISNULL(NON_VESTED_EE_INTEREST,0) AS NON_VESTED_EE_INTEREST,&#xD;&#xA;ISNULL(TOTAL_UVHP_CONTRIBUTION_AMOUNT,0) AS TOTAL_UVHP_CONTRIBUTION_AMOUNT,ISNULL(TOTAL_UVHP_INTEREST_AMOUNT,0) AS TOTAL_UVHP_INTEREST_AMOUNT,&#xD;&#xA;ISNULL(QDRO_OFFSET,0) AS QDRO_OFFSET&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT SPA&#xD;&#xA;INNER JOIN SGT_BENEFIT_CALCULATION_DETAIL CD ON SPA.BENEFIT_CALCULATION_DETAIL_ID = CD.BENEFIT_CALCULATION_DETAIL_ID&#xD;&#xA;WHERE SPA.PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID">
      <parameters>
        <parameter ID="@PAYEE_ACCOUNT_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="GetPaymentCycleDates" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql=" SELECT CONVERT(DATE,PAYMENT_CYCLE_DATE) AS PAYMENT_CYCLE_DATE FROM SGT_PAYMENT_DIRECTIVES WHERE PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID ORDER BY PAYMENT_CYCLE_DATE">
      <parameters>
        <parameter ID="@PAYEE_ACCOUNT_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="GetApprovedDirectivesForPaymentCycle" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT P.*, T.PAYMENT_METHOD FROM SGT_PAYMENT_DIRECTIVES P&#xD;&#xA;INNER JOIN TEMP_TABLE_FOR_PAYMENT T ON P.PAYEE_ACCOUNT_ID = T.PAYEE_ACCOUNT_ID&#xD;&#xA;WHERE IS_DELETED &lt;&gt; 'Y' AND PAYMENT_CYCLE_DATE = @PAYMENT_CYCLE_DATE AND  APPROVED_BY IS NOT NULL &#xD;&#xA;ORDER BY PAYEE_LAST_NAME">
      <parameters>
        <parameter ID="@PAYMENT_CYCLE_DATE" sfwDataType="DateTime" />
      </parameters>
    </query>
    <query ID="RemoveDeletedPaymentAfter36Mnths" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql=" DELETE FROM SGT_PAYMENT_DIRECTIVES WHERE DATEDIFF(MM,PAYMENT_CYCLE_DATE,GETDATE()) &gt; 36 AND ISNULL(IS_DELETED,'N') ='Y'">
      <parameters />
    </query>
    <query ID="PaymentDirectiveReconReport" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="DECLARE @PAYMENTDATE DATETIME&#xD;&#xA;DECLARE @INCLUDE_ADHOC CHAR(1); SET @INCLUDE_ADHOC = 'N'&#xD;&#xA;&#xD;&#xA;IF @PLAN_ID = 1 AND @ADHOC = 1&#xD;&#xA;BEGIN&#xD;&#xA;    SELECT TOP 1 @PAYMENTDATE = CASE WHEN @PLAN_ID = 1 THEN DATEADD(D,7,MAX(PAYMENT_DATE)) ELSE DATEADD(M,1,MAX(PAYMENT_DATE)) END&#xD;&#xA;    FROM SGT_PAYMENT_SCHEDULE  Where (@PLAN_ID = 1 and SCHEDULE_TYPE_VALUE = 'ADHW') and STATUS_VALUE = 'PRCD'&#xD;&#xA;    SET @INCLUDE_ADHOC = 'Y'&#xD;&#xA;END&#xD;&#xA;ELSE&#xD;&#xA;BEGIN&#xD;&#xA;    SELECT TOP 1 @PAYMENTDATE = CASE WHEN @PLAN_ID = 1 THEN DATEADD(D,7,MAX(PAYMENT_DATE)) ELSE DATEADD(M,1,MAX(PAYMENT_DATE)) END&#xD;&#xA;    FROM SGT_PAYMENT_SCHEDULE  Where ((@PLAN_ID &lt;&gt; 1 and SCHEDULE_TYPE_VALUE = 'MTLY') or (@PLAN_ID = 1 and SCHEDULE_TYPE_VALUE = 'WKLY')) and STATUS_VALUE = 'PRCD'&#xD;&#xA;    SET @INCLUDE_ADHOC = 'N'&#xD;&#xA;END&#xD;&#xA;&#xD;&#xA;SELECT A.*,CASE WHEN SP.PERSON_ID IS NULL THEN O.ORG_NAME ELSE SP.FIRST_NAME + ' ' + SP.LAST_NAME END AS PAYEE_NAME, &#xD;&#xA;ISNULL(A.PAYEE_ACCOUNT_GROSS_AMOUNT,0) - ISNULL(A.PAYMENT_DIRECTIVE_GROSS_AMOUNT,0) AS [DIFFERENCE]&#xD;&#xA;FROM &#xD;&#xA;(&#xD;&#xA;  SELECT SPA.PERSON_ID,SPA.ORG_ID,SPA.PAYEE_ACCOUNT_ID,SPAS.STATUS_VALUE,CS.DESCRIPTION AS PAYEE_STATUS,@PAYMENTDATE AS PAYMENT_DATE,XR.PLAN_ID,PL.PLAN_NAME,&#xD;&#xA;  CASE WHEN ISNULL(SPA.RETIREE_INCR_FLAG ,'N') = 'Y' THEN 'Retiree Increase'&#xD;&#xA;       WHEN XR.BENEFIT_OPTION_VALUE = 'LUMP' THEN 'Lump Sum'&#xD;&#xA;       ELSE (CASE WHEN @PLAN_ID = 1 THEN 'Annuity' ELSE 'Monthly' END) END CATEGORY, &#xD;&#xA;  ROUND(SUM(CASE WHEN PIT.ACH_CHECK_GROUP_VALUE = 'GROS' AND ITEM_TYPE_DIRECTION = 1  THEN ((ROUND(ISNULL(PAPI.AMOUNT,0) * (((100-ISNULL(PITY.WITHHOLDING_PERCENTAGE,0))/100)),2))) &#xD;&#xA;        WHEN PIT.ITEM_TYPE_CODE = 'ITEM53' THEN ISNULL(PAPI.AMOUNT,0) * ITEM_TYPE_DIRECTION&#xD;&#xA;        ELSE 0 END),2) &#xD;&#xA;  AS PAYEE_ACCOUNT_GROSS_AMOUNT,&#xD;&#xA;  ROUND(SUM(CASE WHEN  SPAS.STATUS_VALUE = 'RECV' AND PIT.ACH_CHECK_GROUP_VALUE = 'GROS' AND ITEM_TYPE_DIRECTION = 1  THEN ((ROUND(ISNULL(PAPI.AMOUNT,0) * (((100-ISNULL(PITY.WITHHOLDING_PERCENTAGE,0))/100)),2))) &#xD;&#xA;   WHEN SPAS.STATUS_VALUE = 'RECV' AND PIT.ITEM_TYPE_CODE = 'ITEM53' THEN ISNULL(PAPI.AMOUNT,0) * ITEM_TYPE_DIRECTION&#xD;&#xA;    ELSE 0 END) + ISNULL(PD.GROSS_AMOUNT,0) + ISNULL(PD.DIRECT_ROLLOVER,0) + ISNULL(PD.PENSION_RECEIVABLE_AMT,0) + ISNULL(PD.OVER_UNDER_PAYMT_ADJ_AMT,0) ,2) AS PAYMENT_DIRECTIVE_GROSS_AMOUNT,&#xD;&#xA;  CASE WHEN JS.JOB_SCHEDULE_ID IS NOT NULL THEN JS.SCHEDULE_NAME ELSE SPAS.MODIFIED_BY END AS STATUS_MODIFIED_BY,SPAS.MODIFIED_DATE AS STATUS_MODIFIED_DATE,&#xD;&#xA;  PD.CREATED_BY AS PD_CREATED_BY,PD.PAYMENT_DIRECTIVES_ID,(SPP.FIRST_NAME +' '+ SPP.LAST_NAME) PART_NAME&#xD;&#xA;  FROM DBO.SGT_PAYEE_ACCOUNT SPA&#xD;&#xA;  INNER JOIN DBO.SGT_PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE(NOLOCK) PAPI ON SPA.PAYEE_ACCOUNT_ID = PAPI.PAYEE_ACCOUNT_ID &#xD;&#xA;  INNER JOIN DBO.SGT_PAYMENT_ITEM_TYPE(NOLOCK) PIT ON PIT.PAYMENT_ITEM_TYPE_ID = PAPI.PAYMENT_ITEM_TYPE_ID&#xD;&#xA;  INNER JOIN SGT_PAYEE_ACCOUNT_STATUS SPAS ON SPA.PAYEE_ACCOUNT_ID = SPAS.PAYEE_ACCOUNT_ID &#xD;&#xA;   AND SPAS.PAYEE_ACCOUNT_STATUS_ID = (SELECT TOP 1 Q.PAYEE_ACCOUNT_STATUS_ID FROM SGT_PAYEE_ACCOUNT_STATUS Q WHERE Q.PAYEE_ACCOUNT_ID = SPA.PAYEE_ACCOUNT_ID ORDER BY Q.STATUS_EFFECTIVE_DATE DESC)&#xD;&#xA;  INNER JOIN SGS_CODE_VALUE CS ON SPAS.STATUS_ID = CS.CODE_ID AND SPAS.STATUS_VALUE = CS.CODE_VALUE&#xD;&#xA;  INNER JOIN SGT_PLAN_BENEFIT_XR XR ON XR.PLAN_BENEFIT_ID = SPA.PLAN_BENEFIT_ID&#xD;&#xA;  INNER JOIN SGT_PLAN PL ON XR.PLAN_ID = PL.PLAN_ID&#xD;&#xA;  LEFT OUTER JOIN SGT_WITHHOLDING_INFORMATION PITY WITH(NOLOCK) ON SPA.PAYEE_ACCOUNT_ID = PITY.PAYEE_ACCOUNT_ID &#xD;&#xA;    AND PITY.WITHHOLDING_DATE_FROM &lt;= @PAYMENTDATE AND (PITY.WITHHOLDING_DATE_TO IS NULL OR PITY.WITHHOLDING_DATE_TO &gt;= @PAYMENTDATE)                                                           &#xD;&#xA;    AND (PITY.WITHHOLDING_DATE_FROM &lt;&gt; PITY.WITHHOLDING_DATE_TO OR PITY.WITHHOLDING_DATE_TO IS NULL)&#xD;&#xA;  LEFT OUTER JOIN SGT_PAYMENT_DIRECTIVES PD ON SPA.PAYEE_ACCOUNT_ID = PD.PAYEE_ACCOUNT_ID AND CONVERT(DATE,PD.PAYMENT_CYCLE_DATE) = CONVERT(DATE,@PAYMENTDATE) /*AND PD.PLAN_ID &lt;&gt; 1*/ AND PD.APPROVED_DATE IS NOT NULL AND ISNULL(IS_DELETED,'N') = 'N'  --**&#xD;&#xA;  LEFT OUTER JOIN SGT_PAYMENT_DIRECTIVES PDD ON SPA.PAYEE_ACCOUNT_ID = PDD.PAYEE_ACCOUNT_ID AND CONVERT(DATE,PDD.PAYMENT_CYCLE_DATE) = CONVERT(DATE,@PAYMENTDATE) /*AND PDD.PLAN_ID &lt;&gt; 1*/ AND ISNULL(PDD.IS_DELETED,'N') = 'N'  --**&#xD;&#xA;  LEFT OUTER JOIN SGT_PAYEE_BENEFIT_ACCOUNT SPBA WITH(NOLOCK) ON SPBA.PAYEE_BENEFIT_ACCOUNT_ID = SPA.PAYEE_BENEFIT_ACCOUNT_ID &#xD;&#xA;LEFT OUTER JOIN SGT_PERSON_ACCOUNT SPAT WITH(NOLOCK) ON SPAT.PERSON_ACCOUNT_ID = SPBA.PERSON_ACCOUNT_ID &#xD;&#xA;LEFT OUTER JOIN SGT_PERSON SPP WITH(NOLOCK) ON SPP.PERSON_ID =CASE WHEN  SPAT.PERSON_ID IS NULL THEN SPAT.BENEFECIARY_OF_PERSON_ID ELSE SPAT.PERSON_ID END &#xD;&#xA;  LEFT OUTER JOIN (&#xD;&#xA;  SELECT JOB_SCHEDULE_ID,SCHEDULE_NAME FROM SGS_JOB_SCHEDULE&#xD;&#xA;  )JS ON JS.JOB_SCHEDULE_ID = CASE WHEN SPAS.MODIFIED_BY LIKE '%BATCH_%_%' THEN  CAST(SUBSTRING (SPAS.MODIFIED_BY,8 ,2) + '' + CASE WHEN ISNUMERIC(SUBSTRING (SPAS.MODIFIED_BY,10 ,1)) &lt;&gt; 0 THEN SUBSTRING (SPAS.MODIFIED_BY,10 ,1) ELSE ''  END AS INT) ELSE 0 END &#xD;&#xA;  WHERE PAPI.START_DATE &lt;= @PAYMENTDATE AND (PAPI.END_DATE IS NULL OR PAPI.END_DATE &gt;= @PAYMENTDATE) AND (PAPI.START_DATE != ISNULL(PAPI.END_DATE,'99991231'))  &#xD;&#xA;  AND &#xD;&#xA;    ((@PLAN_ID &lt;&gt; 1 AND XR.PLAN_ID &lt;&gt; 1) OR (@PLAN_ID = 1 AND XR.PLAN_ID = 1)) AND (SPAS.STATUS_VALUE IN ('RECV','APRD') OR &#xD;&#xA;            (SPAS.STATUS_VALUE NOT IN ('RECV','APRD')  --**&#xD;&#xA;            AND (PDD.PAYMENT_DIRECTIVES_ID IS NOT NULL OR EXISTS(SELECT 1 FROM TEMP_TABLE_FOR_PAYMENT T WHERE ISNULL(REISSUE_ITEM_FLAG,'N') = 'N' --AND ISNULL(INCLUDE_IN_ADHOC_FLAG,'N') = 'N' &#xD;&#xA;                        AND ((@PLAN_ID &lt;&gt; 1 AND PLAN_ID &lt;&gt; 1) OR (@PLAN_ID = 1 AND PLAN_ID = 1)) AND T.PAYEE_ACCOUNT_ID = SPA.PAYEE_ACCOUNT_ID)) )&#xD;&#xA;    )&#xD;&#xA;    AND ISNULL(INCLUDE_IN_ADHOC_FLAG,'N') = @INCLUDE_ADHOC&#xD;&#xA;  GROUP BY SPAS.MODIFIED_DATE,PL.PLAN_NAME,SPA.PAYEE_ACCOUNT_ID,PD.PAYMENT_DIRECTIVES_ID,CS.DESCRIPTION ,SPA.RETIREE_INCR_FLAG,XR.BENEFIT_OPTION_VALUE,SPA.PERSON_ID,SPA.ORG_ID,&#xD;&#xA;  PD.GROSS_AMOUNT,PD.DIRECT_ROLLOVER,SPAS.MODIFIED_BY,PD.OVER_UNDER_PAYMT_ADJ_AMT,XR.PLAN_ID,SPAS.STATUS_VALUE,PD.CREATED_BY ,PD.PENSION_RECEIVABLE_AMT,SPP.FIRST_NAME,  SPP.LAST_NAME ,JS.JOB_SCHEDULE_ID ,JS.SCHEDULE_NAME&#xD;&#xA;)A&#xD;&#xA;LEFT OUTER JOIN SGT_PERSON SP ON A.PERSON_ID = SP.PERSON_ID&#xD;&#xA;LEFT OUTER JOIN SGT_ORGANIZATION O ON A.ORG_ID = O.ORG_ID&#xD;&#xA;ORDER BY A.STATUS_VALUE,A.PAYEE_ACCOUNT_ID">
      <parameters>
        <parameter ID="@PLAN_ID" sfwDataType="int" />
        <parameter ID="@ADHOC" sfwDataType="int" />
      </parameters>
    </query>
  </queries>
