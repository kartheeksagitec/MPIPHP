                    </TemplateField>
                    <TemplateField HeaderText="Federal Tax">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="IdecFederalTax" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="State Tax">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="IdecStateTax" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Deduction Amount">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="IdecDeductionAmt" />
