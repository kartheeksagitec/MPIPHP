              <sfwColumn>
                <sfwLabel AssociatedControlID="txtSpaPayeeAccountId" ID="capSpaPayeeAccountId" Text="Retiree Increase Payee Account ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtSpaPayeeAccountId" sfwDataField="payee_account_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoPayeeAccount" />
                <sfwImageButton ID="btnRetrieve1" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPayeeAccountLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtSpaPayeeAccountId" sfwSelectedIndex="PayeeAccountId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtReferenceId" ID="capReferenceId" Text="Monthly Payee Account ID : " sfwIsCaption="True" />
