                <sfwLabel AssociatedControlID="txtSpMpiPersonId" ID="capSpMpiPersonId" Text="Payee MPID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtSpMpiPersonId" sfwDataField="mpi_person_id" sfwOperator="like" sfwQueryID="cdoPayeeAccount" sfwTwoWayBinding="False" />
                <sfwImageButton ID="btnRetrieve" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtSpMpiPersonId" sfwSelectedIndex="MpiPersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtParticipantMpiId" ID="capParticipantMpiId" Text="Participant MPID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtParticipantMpiId" sfwDataField="participant_mpi_id" sfwOperator="like" sfwQueryID="cdoPayeeAccount" sfwTwoWayBinding="False" />
                <sfwImageButton ID="btnRetrieve2" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtParticipantMpiId" sfwSelectedIndex="MpiPersonId" />
              </sfwColumn>
