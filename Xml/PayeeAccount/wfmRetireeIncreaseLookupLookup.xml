                        <sfwLinkButton sfwActiveForm="wfmPayeeAccountMaintenance" sfwDataField="icdoPayeeAccount.reference_id" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPayeeAccountId=ReferenceId" sfwRelatedControl="dgrResult" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Monthly Payee Account Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="IstrMonthlyPayeeAccountCurrentStatus" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
