              <sfwColumn>
                <sfwTextBox ID="txtReferenceId" sfwDataField="reference_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoPayeeAccount" sfwTwoWayBinding="False" />
                <sfwImageButton ID="btnRetrieve3" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPayeeAccountLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtReferenceId" sfwSelectedIndex="PayeeAccountId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="ddlPlnPlanId" ID="capPlnPlanId" Text="Plan ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="plan_name" DataValueField="plan_id" ID="ddlPlnPlanId" sfwDataField="plan_id" sfwOperator="=" sfwQueryID="cdoPayeeAccount" sfwLoadType="Query" sfwLoadSource="entPlan.Lookup" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtRetrIncrYear" ID="capRetrIncrYear" Text="Retiree Increase Year : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtRetrIncrYear" sfwDataField="TEMP.year" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoPayeeAccount" sfwTwoWayBinding="False" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="ddlSpasStatusValue" ID="capSpasStatusValue" Text="Retiree Increase Payee Account Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlSpasStatusValue" sfwDataField="status_value" sfwOperator="like" sfwQueryID="cdoPayeeAccount" sfwLoadType="CodeGroup" sfwLoadSource="7027" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="ddlStatusValue1" ID="capStatusValue1" Text="Monthly Payee Account Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlStatusValue1" sfwDataField="pstatus_value" sfwOperator="like" sfwQueryID="cdoPayeeAccount" sfwTwoWayBinding="False" sfwLoadType="CodeGroup" sfwLoadSource="7027" />
