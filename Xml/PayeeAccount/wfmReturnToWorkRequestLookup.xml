    <query ID="entReturnToWorkRequest" sfwQueryRef="entReturnToWorkRequest.LookUp" sfwMethodName="LoadReturnToToWorkRequest" />
  </initialload>
  <sfwTable>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblReqInformationMpiPersonId" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capSpMpiPersonId" Text="MPID : " AssociatedControlID="txtSpMpiPersonId" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtSpMpiPersonId" sfwDataField="sp.mpi_person_id" sfwOperator="like" sfwQueryID="entReturnToWorkRequest" sfwCustomAttributes="maxlength=20" />
                <sfwImageButton ID="btnRetrievePayee" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtSpMpiPersonId" sfwSelectedIndex="MpiPersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capUnionLocalNumber" Text="Union Local Number : " AssociatedControlID="txtUnionLocalNumber" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtUnionLocalNumber" sfwDataField="srw.union_local_number" sfwDataType="Numeric" sfwMaxValue="9999" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStatusValue" Text="Status : " AssociatedControlID="ddlStatusValue" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlStatusValue" sfwDataField="srw.status_value" sfwOperator="like" sfwLoadType="CodeGroup" sfwLoadSource="7099" DataTextField="description" DataValueField="code_value" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capReemploymentStartDate" Text="From Effective Date : " AssociatedControlID="txtReemploymentStartDate" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtReemploymentStartDate" sfwDataField="srw.reemployment_start_date" sfwDataType="DateTime" sfwDataFormat="{0:d}" sfwOperator="between" sfwRelatedControl="txtReemploymentStartDateTo" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capReemploymentStartDatetTo" Text="To Effective Date : " AssociatedControlID="txtReemploymentStartDateTO" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtReemploymentStartDateTo" sfwDataField="srw.reemployment_start_date" sfwDataType="DateTime" sfwDataFormat="{0:d}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblReqInformationSourceValue" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capSourceValue" Text="Source : " AssociatedControlID="ddlSourceValue" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlSourceValue" sfwDataField="srw.source_value" sfwOperator="like" sfwLoadType="CodeGroup" sfwLoadSource="8000" DataTextField="description" DataValueField="code_value" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capEstimatedHoursPerPayrollMonth" Text="Estimated Hours Per Payroll Month : " AssociatedControlID="txtEstimatedHoursPerPayrollMonth" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtEstimatedHoursPerPayrollMonth" sfwDataField="srw.estimated_hours_per_payroll_month" sfwDataType="Decimal" sfwMaxValue="999.99" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capJobClassfication" Text="Job Classfication : " AssociatedControlID="txtJobClassfication" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtJobClassfication" sfwDataField="srw.job_classfication" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblReqInformationTypeValue" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capTypeValue" Text="Request Type : " AssociatedControlID="ddlTypeValue" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlTypeValue" sfwDataField="srw.request_type_value" sfwOperator="like" sfwLoadType="CodeGroup" sfwLoadSource="8001" DataTextField="description" DataValueField="code_value" sfwDefaultValue="RTWO" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn />
              <sfwColumn />
              <sfwColumn>
                <sfwLabel ID="capEmployerName" Text="Employer Name : " AssociatedControlID="txtEmployerName" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtEmployerName" sfwDataField="srw.employer_name" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" sfwOperateOn="tblCriteria" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" sfwOperateOn="tblCriteria" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" sfwOperateOn="tblCriteria" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable ID="tblResult" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" ImageUrl="~/Image/gNew.gif" sfwMethodName="btnNew_Click" sfwOperateOn="tblCriteria" Text="New" sfwActiveForm="wfmReturnToWorkRequestMaintenance" sfwNavigationParameter="aintMPIPersonId=txtSpMpiPersonId;aintRequestTypeValue=ddlTypeValue;aintSourceValue=ddlSourceValue" sfwCopyActivitySessionVariables="True" sfwResource="1101" />
