              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView ID="dgrResult" AllowPaging="True" AllowSorting="True" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ReemploymentNotificationId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Union Local Number" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="UnionLocalNumber" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="MPID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="objPerson.MpiPersonId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Person Name">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwEntityField="objPerson.IstrFullName" sfwRelatedControl="btnOpenPerson" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Re-employment Start Date">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ReemploymentStartDate" sfwDataFormat="{0:d}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Source" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="SourceDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Estimated Hours Per Payroll Month">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="EstimatedHoursPerPayrollMonth" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Job Classfication" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="JobClassfication" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Employer Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="EmployerName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Person ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PersonId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Request">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="RequestTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
