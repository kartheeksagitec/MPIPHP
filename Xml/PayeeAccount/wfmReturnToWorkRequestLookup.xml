<form ID="wfmReturnToWorkRequestLookup" sfwType="Lookup" sfwEntity="entReturnToWorkRequest" sfwTitle="Return To Work Request Lookup" sfwActive="True" sfwStatus="Review" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmReturnToWorkRequestLookup" sfwRemoteObject="srvPayeeAccount" sfwGridCollection="busReturnToWorkRequestLookup.iclbReturnToWorkRequest" sfwResource="1100">
