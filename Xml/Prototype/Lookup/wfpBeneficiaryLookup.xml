<?xml version="1.0" encoding="utf-8"?>
<form ID="wfpBeneficiaryLookup" sfwActive="True" sfwStatus="Review" sfwType="Lookup" sfwTitle="Beneficiary Lookup" version="3.5" sfwEntity="entPrototype">
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtLastname" ID="capLastname" Text="Last Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtLastname" sfwOperator="like" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtFirstname" ID="capFirstname" Text="First Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtFirstname" sfwDataField="first_name" sfwOperator="like" sfwQueryID="cdoPerson" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblLabel1" Text="Last 4 Digits of SSN" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtSSN" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtDateofbirth" ID="capDateofbirth" Text="Date of Birth : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtDateofbirth" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="=" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtPersonid" ID="capPersonid" Text="PIN : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonid" sfwOperator="like" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnPrototypeSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfpBeneficiaryMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfpBeneficiaryMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="form_name=#wfpBeneficiaryLookup;grid_view=#dgrResult;row_index=#Index" sfwRelatedControl="dgrResult" Text="Open" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwObjectID="busPrototypeLookup.iclbPrototype" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="PIN" SortExpression="icdoPerson.mpi_person_id">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="Property0" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Full Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="Property1" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Last 4 Digits of SSN">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="Property2" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Date of Birth">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="Property3" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                  <Data>
                    <DataRow>
                      <DataColumn Value="M1" sfwEntityField="Property0" />
                      <DataColumn Value="Jane Smith" sfwEntityField="Property1" />
                      <DataColumn Value="4321" sfwEntityField="Property2" />
                      <DataColumn Value="01/21/1948" sfwEntityField="Property3" />
                    </DataRow>
                    <DataRow>
                      <DataColumn Value="M2" sfwEntityField="Property0" />
                      <DataColumn Value="John Doe" sfwEntityField="Property1" />
                      <DataColumn Value="8123" sfwEntityField="Property2" />
                      <DataColumn Value="01/08/1966" sfwEntityField="Property3" />
                    </DataRow>
                  </Data>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
</form>